public String decodeString(ArrayList<StringPart>parts){
  for(int i=0;i<parts.size();i++){
    String a="";
    String a += masterString.substring(i.getStart(),i.getStart() + i.getLength())
    }
    return a;
 }
 public ArrayList<StringPart> ecodeString(String word){
    ArrayList<StringPart> a = new ArrayList<StringPart>();
      while(word.length()!=0){
      StringPart part = findPart(word);
      a.add(part);
      word = word.substring(part.getLength());
      }
      return a;
  }

  
